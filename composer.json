    "keywords": ["aoc","advent of code", "aoc helper", "advent of code helper"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mjderoode\\AdventOfCodeHelper\\": "src/"
        }
    },
    "authors": [
        {
